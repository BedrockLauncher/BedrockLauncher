<Grid x:Class="BedrockLauncher.Controls.Various.ResultPanel"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BedrockLauncher"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Background="#FF323232" Loaded="Grid_Loaded">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <TextBlock Foreground="{DynamicResource PrimaryText.Foreground}" 
                   Name="Text1"
                   TextAlignment="Center" 
                   Text="Text1"
                   Margin="0,0,0,10"
                   FontSize="26"
                   FontWeight="SemiBold"></TextBlock>
        <TextBlock Foreground="{DynamicResource PrimaryText.Foreground}"
                   Name="Text2"
                   TextAlignment="Center" 
                   Text="Text2"></TextBlock>
    </StackPanel>
</Grid>
